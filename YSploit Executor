-- Scripts:

local function DQDZ_fake_script() -- exec.LocalScript 
	local script = Instance.new('LocalScript', exec)

	script.Parent.Activated:Connect(function()
		loadstring(script.Parent.Parent.funni.Text)()
	end)
end
coroutine.wrap(DQDZ_fake_script)()
local function ODIHEOF_fake_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.funni.Text = ""
	end)
end
coroutine.wrap(ODIHEOF_fake_script)()
local function TLXX_fake_script() -- fard.LocalScript 
	local script = Instance.new('LocalScript', fard)

	script.Parent.Activated:Connect(function()
		local bazinga = Instance.new("Sound")
		bazinga.Parent = script.Parent
		bazinga.SoundId = 18728158453
		bazinga.Playing = true
	end)
end
coroutine.wrap(TLXX_fake_script)()
local function BGSOX_fake_script() -- clip.LocalScript 
	local script = Instance.new('LocalScript', clip)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.funni.Text = getclipboard()
		wait(0.2)
		loadstring(script.Parent.Parent.Parent.funni.Text)()
	end)
end
coroutine.wrap(BGSOX_fake_script)()
local function KULJGKI_fake_script() -- MainUI.LocalScript 
	local script = Instance.new('LocalScript', MainUI)

	local sheldon = script.Parent
	
	sheldon.Draggable = true
	sheldon.Active = true
end
coroutine.wrap(KULJGKI_fake_script)()
local function NMJVD_fake_script() -- xfrombfdi.LocalScript 
	local script = Instance.new('LocalScript', xfrombfdi)

	local me = script.Parent.Parent.Parent.MainUI
	local main = script.Parent.Parent.Parent.ClosedUI
	
	
	script.Parent.Activated:Connect(function()
		me.Visible = false
		main.Visible = true
	end)
	
end
coroutine.wrap(NMJVD_fake_script)()
local function XLAP_fake_script() -- ClosedUI_2.LocalScript 
	local script = Instance.new('LocalScript', ClosedUI_2)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.MainUI.Visible = true
	end)
	
end
coroutine.wrap(XLAP_fake_script)()
local function RCRYF_fake_script() -- ClosedUI.LocalScript 
	local script = Instance.new('LocalScript', ClosedUI)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(RCRYF_fake_script)()
